{
  "id": "window",
  "label": "Coastal Blue Carbon",
  "targetScript": "natcap.invest.coastal_blue_carbon.coastal_blue_carbon",
  "modelName": "coastal_blue_carbon",
  "localDocURI": "../documentation/coastal_blue_carbon.html",
  "height": 600,
  "width": 700,
  "elements": [
    {
      "id": "workspace_list",
      "type": "list",
      "elements": [
        {
          "id": "workspace_dir",
          "args_id": "workspace_dir",
          "type": "folder",
          "label": "Workspace",
          "defaultValue": "../CoastalBlueCarbon/",
          "helpText": "The selected folder is used as the workspace where all intermediate and output files will be written.<br><br>If the selected folder does not exist, it will be created.<br><br><b>If datasets already exist in the selected folder, they will be overwritten.</b><p>For more information, see 'Data Needs' section in the model's documentation",
          "required": true
        },
        {
          "id": "results_suffix",
          "args_id": "results_suffix",
          "type": "text",
          "dataType": "string",
          "label": "Results Suffix (Optional)",
          "defaultValue": "",
          "required": false,
          "helpText": "(help text)"
        },
        {
          "id": "lulc_transition_uri",
          "args_id": "lulc_transition_uri",
          "type": "file",
          "label": "LULC Transition Effect of Carbon Table (CSV)",
          "defaultValue": "../CoastalBlueCarbon/inputs/transition.csv",
          "helpText": "see 'Data Needs' section in the model's documentation",
          "required": true
        },
        {
          "id": "carbon_pool_initial_uri",
          "args_id": "carbon_pool_initial_uri",
          "type": "file",
          "label": "Carbon Pools Initial Variables Table (CSV)",
          "defaultValue": "../CoastalBlueCarbon/inputs/carbon_pools_initial.csv",
          "helpText": "see 'Data Needs' section in the model's documentation",
          "required": true
        },
        {
          "id": "carbon_pool_transient_uri",
          "args_id": "carbon_pool_transient_uri",
          "type": "file",
          "label": "Carbon Pools Transient Variables File (CSV)",
          "defaultValue": "../CoastalBlueCarbon/inputs/carbon_pools_transient.csv",
          "helpText": "",
          "required": true
        }
      ]
    },
    {
      "id": "lulc_snapshot_list",
      "type": "multi",
      "args_id": "lulc_snapshot_list",
      "label": "Land Use/Land Cover Rasters (GDAL-supported)",
      "sampleElement": {
        "id": "sample_id",
        "type": "file",
        "label": "Input",
        "validateAs": {
          "type": "GDAL"
        }
      },
      "defaultValue": [
        "../CoastalBlueCarbon/lulc_rasters/GBJC_2004_mean_Resample.tif",
        "../CoastalBlueCarbon/lulc_rasters/GBJC_2050_mean_Resample.tif",
        "../CoastalBlueCarbon/lulc_rasters/GBJC_2100_mean_Resample.tif"
      ],
      "linkText": "Add Another",
      "helpText": "(help text)"
    },
    {
      "id": "lulc_snapshot_years_list",
      "type": "multi",
      "args_id": "lulc_snapshot_years_list",
      "label": "Land Use/Land Cover Years",
      "sampleElement": {
        "id": "sample_id",
        "type": "file",
        "label": "Input",
        "validateAs": {
          "type": "number",
          "allowedValues": {
             "pattern": "^[0-9]+"
          }
        }
      },
      "defaultValue": [
        2004,
        2050,
        2100
      ],
      "linkText": "Add Another",
      "helpText": "(help text)"
    }*/
